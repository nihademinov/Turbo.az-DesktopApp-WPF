    <Style TargetType="Button" x:Key="VehicleViewButton">
        <Setter Property="Background" Value="Transparent"/>



        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Border Background="White" BorderBrush="LightGray" BorderThickness="2" CornerRadius="5" >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="7*"/>
                                <RowDefinition Height="1.4*"/>
                                <RowDefinition Height="1*"/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Image Source="{Binding photos[0]}" HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="UniformToFill" />
                                <Button Style="{StaticResource favoritiesButtonOnVehicleButton}" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,10,15,0" Width="22" >
                                    <Image Source="/StaticFiles/PageImages/GrayHeartVehicleBtn.png" Width="30" Height="30"/>
                                </Button>

                            </Grid>
                            <Grid Grid.Row="1">
                                <Label Content="{Binding price}" FontSize="16" FontWeight="Bold" HorizontalAlignment="Left" Margin="5,0,0,0"/>
                            </Grid>



                            <Grid Grid.Row="2">

                                <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
                                    <Label Content="{Binding maker}" HorizontalAlignment="Left" Margin="5,-5,0,0" FontSize="15" FontWeight="DemiBold" />
                                    <Label Content="{Binding model}" HorizontalAlignment="Left" FontSize="15" FontWeight="DemiBold" Margin="0,-5,0,0"/>
                                </StackPanel>
                            </Grid>



                            <Grid Grid.Row="3">
                                <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
                                    <Label Content="{Binding year}" HorizontalAlignment="Left" Margin="5,-5,0,0" FontSize="13"  />
                                    <Label Content="," FontSize="15" Margin="-7,-5,0,-3" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                    <Label Content="{Binding engineVolume}" HorizontalAlignment="Left" Margin="-9,-5,0,0" FontSize="13"  />
                                    <Label Content="," FontSize="15" Margin="-7,-5,0,-3"/>
                                    <Label Content="{Binding mileage}" HorizontalAlignment="Left" Margin="-9,-5,0,0" FontSize="13"  />



                                </StackPanel>
                                <!--<Label Content="test 3"/>-->
                            </Grid>







                        </Grid>



                    </Border>
                    <ControlTemplate.Triggers>

                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>



        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Cursor" Value="Hand"/>
            </Trigger>





        </Style.Triggers>
    </Style>



