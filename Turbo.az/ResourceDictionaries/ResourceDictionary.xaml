                                </StackPanel>
                            </Grid>

                            <Grid Grid.Row="3">
                                <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
                                    <Label Content="{Binding year}" HorizontalAlignment="Left" Margin="5,-5,0,0" FontSize="13"  />
                                    <Label Content="," FontSize="15" Margin="-7,-5,0,-3" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                    <Label Content="{Binding engineVolume}" HorizontalAlignment="Left" Margin="-9,-5,0,0" FontSize="13"  />
                                    <Label Content="," FontSize="15" Margin="-7,-5,0,-3"/>
                                    <Label Content="{Binding mileage}" HorizontalAlignment="Left" Margin="-9,-5,0,0" FontSize="13"  />
                                </StackPanel>
                                <!--<Label Content="test 3"/>-->
                            </Grid>
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>

                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

